#include <stdio.h>

struct student {
    char name[30];
    char roll_no[10];
    int marks;
};

int main() {
    struct student s1[5];
    for (int i = 0; i < 5; i++) {
        printf("Enter name: ");
        scanf("%29s", s1[i].name);
        printf("Enter roll_no: ");
        scanf("%9s", s1[i].roll_no);
        printf("Enter marks: ");
        scanf("%d", &s1[i].marks);
    }
    return 0;
}
